// STAR PATTERN PRINT THE LINES WHICH IS EQUAL TO NUMBER OF LINES
*
**
***
****
#include <stdio.h>
int main(){
    int n;
    printf("enter the value of n\n");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        for(int j=0;j<i+1;j++){//as number of stars increasing in next lines
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
